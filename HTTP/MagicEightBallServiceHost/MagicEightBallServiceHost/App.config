<configuration>
  <system.serviceModel>
    <services>
      <service name="MagicEightBallService.MagicEightBallService">
        <host>
          <baseAddresses>
            <add baseAddress="http://localhost:8080/MagicEightBallService"/>
            <add baseAddress="net.tcp://localhost:8099/MagicEightBallService"/>
          </baseAddresses>
        </host>
      </service>
    </services>
    <bindings>
      <basicHttpBinding>
        <binding openTimeout="00:00:30"/>
      </basicHttpBinding>
      <netTcpBinding>
        <binding closeTimeout="00:00:15"/>
      </netTcpBinding>
    </bindings>
    <behaviors>
      <serviceBehaviors>
        <behavior>
          <!-- To get default MEX,
donâ€™t name your <serviceMetadata> element -->
          <serviceMetadata httpGetEnabled="true"/>
        </behavior>
      </serviceBehaviors>
    </behaviors>
  </system.serviceModel>
<startup><supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.8"/></startup></configuration>
